
{
    "parser" : "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    'extends' : 'eslint:recommended',
    'env' : {
            'node' : 'true',
            'mocha' : 'true'
	},
    "rules": 
    {
        "semi": 2,
        "no-console": 1,
	"no-extra-semi": 2,
	"no-unreachable": 2
    }
}
